{
  "expo": {
    "name": "Smoki - by Maślak",
    "slug": "SmokiApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smokiapp.game"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF6B35"
      },
      "package": "com.smokiapp.game",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Smoki - by Maślak",
      "shortName": "Smoki",
      "lang": "pl",
      "scope": "/",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#FF6B35",
      "backgroundColor": "#FF6B35",
      "bundler": "webpack"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
